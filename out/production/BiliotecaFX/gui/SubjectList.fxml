<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="371.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SubjectListController">
    <Label prefHeight="31.0" prefWidth="162.0" text="Subject Registration" textAlignment="CENTER">
        <font>
          <Font name="System Bold" size="14.0" />
        </font>
        <padding>
          <Insets left="5.0" top="5.0" />
        </padding>
    </Label>
    <Button fx:id="btNew" mnemonicParsing="false" onAction="#onBtNewAction" text="New">
      <VBox.margin>
         <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin></Button>
    <TableView fx:id="tableViewSubject" prefHeight="200.0" prefWidth="200.0">
      <columns>
          <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="tableColumnName" prefWidth="75.0" text="Name" />
      </columns>
    </TableView>
</VBox>
